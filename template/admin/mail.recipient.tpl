<tr class="tr-mail-recipient">
    <td class="recipient-selection">
        <input type="checkbox" class="recipient-checked" value="${name}$" />
    </td>
    <td class="recipient-name">${name}$</td>
    <td class="recipient-email">${address}$</td>
    <td class="recipient-plans">
        ${plans}$
    </td>
</tr>