<tr class="tr-shift">
    <td class="tr-shift-uid readonly td-debug">${id}$</td>
    <td class="tr-shift-from" style="width:50%;">
        <div class="bfh-timepicker" data-time="${from}$"></div>
    </td>
    <td class="tr-shift-to" style="width:50%;">
        <div class="bfh-timepicker" data-time="${to}$"></div>
    </td>
    <td class="tr-shift-delete readonly"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></td>
</tr>