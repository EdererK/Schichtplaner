<div class="worker ${fixed}$" data-name="${name}$" data-email="${email}$">
	<div class="user-name">
        <span class="glyphicon glyphicon-question-sign not-fixed-hint" aria-hidden="true"></span>
        <span class="glyphicon glyphicon-ok-sign is-fixed-hint" aria-hidden="true"></span>
        <span class="top glyphicon glyphicon-exclamation-sign missing-email-hint" aria-hidden="true" style="font-size:12px; color:#333;" title="E-Mail Adresse fehlt" data-original-title="E-Mail Adresse fehlt"></span>
        <span class="name-inline">${name}$</span>
    </div>
	<div class="user-email">${email}$</div>
</div>